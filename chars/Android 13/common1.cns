physics = C
anim = 10+IfElse(Var(3)= 1,11000,ifelse(Var(2)=1, 10000,0))

[State 10, 1]
type = VelMul
trigger1 = Time = 0
x = .75

[State 10, 2]
type = ChangeState
trigger1 = AnimTime = 0
value = 11

;---------------------------------------------------------------------------
; Crouching
[Statedef 11]
type    = C
physics = C
anim = 11+IfElse(Var(3)= 1,11000,ifelse(Var(2)=1, 10000,0))
sprpriority = 0
velset = (PrevStateno = 9000)* 0,(PrevStateno = 9000)* 0
facep2 = !(PrevStateno = 9000)
ctrl = 1

;[State 11, 1] ;Change from turning animation
;type = ChangeAnim
;trigger1 = Anim = 6 && AnimTime = 0
;value = 11

[State 0, 1]
type = ChangeAnim
trigger1 = (Anim != 11 && Anim != 6 && Anim != 10011 && Anim != 10006 && Anim != 11011 && Anim != 11006)
trigger2 = (Anim = 6 || anim = 10006 || anim = 11006) && AnimTime = 0
value = 11+IfElse(Var(3)= 1,11000,ifelse(Var(2)=1, 10000,0))


[State 0, ChangeAnim]
type = ChangeAnim
triggerall = (var(3) = 1)
trigger1 = anim = 6
value = 11006


[State 0, ChangeAnim]
type = ChangeAnim
triggerall = (var(2) = 1)
trigger1 = anim = 6
value = 10006

