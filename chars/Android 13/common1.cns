value = 0

;---------------------------------------------------------------------------
; Walk
[Statedef 20]
type    = S
physics = S
sprpriority = 0

[State 20, 1]
type = VelSet
trigger1 = command = "holdfwd"
x = ifelse(var(3) = 1, const(velocity.walk.fwd.x)-0.7 ,const(velocity.walk.fwd.x))

[State 20, 2]
type = VelSet
trigger1 = command = "holdback"
x = ifelse(var(3) = 1,const(velocity.walk.back.x)+0.7, const(velocity.walk.back.x))


[State 0, PlaySnd]
type = PlaySnd
triggerall = var(3) = 1
trigger1 = Animelem = 3
value = S40,1
volume = 100

[State 0, PlaySnd]
type = PlaySnd
triggerall = var(4) = 1 || var(2) = 1
trigger1 = Animelem = 3
value = S40,0
volume = 100

[State 20, 3]
type = ChangeAnim
triggerall = vel x > 0
trigger1 = (Anim != 20 && Anim != 5 && Anim != 10020 && Anim != 10005 && Anim != 11020 && Anim != 11005)
trigger2 = (Anim = 5 || anim = 10005 || anim = 11005) && AnimTime = 0
value = 20+IfElse(Var(3)= 1,11000,ifelse(Var(2)=1, 10000,0))

[State 20, 4]
type = ChangeAnim
triggerall = vel x < 0
trigger1 = (Anim != 21 && Anim != 5 && Anim != 10021 && Anim != 10005 && Anim != 11021 && Anim != 11005)
trigger2 = (Anim = 5 || anim = 10005 || anim = 11005) && AnimTime = 0
value = 21+IfElse(Var(3)= 1,11000,ifelse(Var(2)=1, 10000,0))

;---------------------------------------------------------------------------
; Jump Start
[Statedef 40]
type    = S
physics = S
anim = 40+IfElse(Var(3)= 1,11000,ifelse(Var(2)=1, 10000,0))
ctrl = 0
sprpriority = 1

[State 40, 1]
type = VarSet
trigger1 = Time = 0
sysvar(1) = 0

[State 40, 2]
type = VarSet
trigger1 = command = "holdfwd"
sysvar(1) = 1

[State 40, 3]
type = VarSet
trigger1 = command = "holdback"
sysvar(1) = -1

[State 40, 4]
type = VelSet
trigger1 = AnimTime = 0
x = ifelse(sysvar(1)=0, const(velocity.jump.neu.x), ifelse(sysvar(1)=1, const(velocity.jump.fwd.x)-(ifelse(var(3) = 1,0.7,0)), const(velocity.jump.back.x)+(ifelse(var(3) = 1,0.7,0))))
y = const(velocity.jump.y)+(ifelse(var(3) = 1,0.7,0))



[State 40, 5]
type = VelSet
trigger1 = AnimTime = 0
trigger1 = prevstateno = 100 ;RUN_FWD
trigger1 = sysvar(1) = 1
x = const(velocity.runjump.fwd.x)

[State 40, 6]
type = ChangeState
trigger1 = AnimTime = 0
value = 50
ctrl = 1

;---------------------------------------------------------------------------
; AirJump Start
[Statedef 45]
type    = A
physics = N
ctrl = 0
velset = 0,0

[State 45, 1]
type = ChangeAnim
trigger1 = SelfAnimExist(44)
value = 44+IfElse(Var(3)= 1,11000,ifelse(Var(2)=1, 10000,0))

[State 45, 2]
type = ChangeAnim
trigger1 = !SelfAnimExist(44)
value = 41+IfElse(Var(3)= 1,11000,ifelse(Var(2)=1, 10000,0))

[State 45, 3]
type = VarSet
trigger1 = Time = 0
sysvar(1) = 0

[State 45, 4]
type = VarSet
trigger1 = command = "holdfwd"
sysvar(1) = 1

[State 45, 5]
type = VarSet
trigger1 = command = "holdback"
sysvar(1) = -1

[State 45, 6]
type = VelSet
trigger1 = Time = 2
x = ifelse(sysvar(1)=0, const(velocity.airjump.neu.x), ifelse(sysvar(1)=1, const(velocity.airjump.fwd.x), const(velocity.airjump.back.x)))
y = const(velocity.airjump.y)

[State 45, 7]
type = ChangeState
trigger1 = Time = 2
value = 50
ctrl = 1

;---------------------------------------------------------------------------
; Jump Up
[Statedef 50]
type    = A
physics = A

[State 50, 1]
type = VarSet
trigger1 = Time = 0
sysvar(1) = 0

[State 50, 2]
type = ChangeAnim
trigger1 = Time = 0
value = ifelse((vel x)=0, 41+IfElse(Var(3)= 1,11000,ifelse(Var(2)=1, 10000,0)), ifelse((vel x)>0, 42+IfElse(Var(3)= 1,11000,ifelse(Var(2)=1, 10000,0)), 43+IfElse(Var(3)= 1,11000,ifelse(Var(2)=1, 10000,0))))

[State 50, 3]
type = ChangeAnim
trigger1 = Vel y > -2
trigger1 = SelfAnimExist(anim + 3)
persistent = 0
value = Anim + 3

