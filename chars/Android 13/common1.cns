; HITA_FALL (knocked up, falling)
[Statedef 5050]
type    = A
movetype= H
physics = N

[State 5050, 1] ;Change anim when done with transition
type = ChangeAnim
trigger1 = AnimTime = 0
trigger1 = Anim = 5035+IfElse(Var(3)= 1,11000,ifelse(Var(2) = 1, 10000,0))
trigger2 = Time = 0     ;If no transition anim
trigger2 = Anim != 5035+IfElse(Var(3)= 1,11000,ifelse(Var(2) = 1, 10000,0))
trigger2 = (Anim != [5051, 5059])+IfElse(Var(3)= 1,11000,ifelse(Var(2) = 1, 10000,0)) && (Anim != [5061, 5069])+IfElse(Var(3)= 1,11000,ifelse(Var(2) = 1, 10000,0))
trigger2 = Anim != 5090+IfElse(Var(3)= 1,11000,ifelse(Var(2) = 1, 10000,0)) ;Not if hit off ground anim
value = 5050+IfElse(Var(3)= 1,11000,ifelse(Var(2) = 1, 10000,0))

[State 5050, 2] ;Coming down anim
type = ChangeAnim
trigger1 = (anim = [5050,5059])+IfElse(Var(3)= 1,11000,ifelse(Var(2) = 1, 10000,0))
trigger1 = Vel Y >= ifelse(anim = 5050+IfElse(Var(3)= 1,11000,ifelse(Var(2) = 1, 10000,0)), 1, -2)
trigger1 = SelfAnimExist(anim+10)
value = anim+10
persistent = 0

[State 5050, 3] ;Gravity
type = VelAdd
trigger1 = 1
y = GetHitVar(yaccel)

[State 5050, 4] ;Recover near ground
type = ChangeState
triggerall = Vel Y > 0
triggerall = Pos Y >= -20
triggerall = alive
triggerall = CanRecover
trigger1 = Command = "recovery"
value = 5200 ;HITFALL_RECOVER

[State 5050, 5]; Recover in mid air
type = ChangeState
triggerall = Vel Y > -1
triggerall = alive
triggerall = CanRecover
trigger1 = Command = "recovery"
value = 5210 ;HITFALL_AIRRECOVER

[State 5050, 6]
type = ChangeState
trigger1 = Vel Y > 0
trigger1 = Pos Y >= ifelse((anim = [5051,5059])+IfElse(Var(3)= 1,11000,ifelse(Var(2) = 1, 10000,0)) || (anim = [5061,5069])+IfElse(Var(3)= 1,11000,ifelse(Var(2) = 1, 10000,0)), 0, 25)
value = 5100 ;HIT_BOUNCE

